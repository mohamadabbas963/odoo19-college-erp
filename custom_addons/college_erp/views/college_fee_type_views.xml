<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_college_fee_type_form" model="ir.ui.view">
        <field name="name">college.fee.type.form</field>
        <field name="model">college.fee.type</field>
        <field name="arch" type="xml">
            <form><sheet><group><field name="name"/></group></sheet></form>
        </field>
    </record>

    <record id="view_college_fee_type_tree" model="ir.ui.view">
        <field name="name">college.fee.type.tree</field>
        <field name="model">college.fee.type</field>
        <field name="arch" type="xml">
            <list><field name="name"/></list>
        </field>
    </record>

    <record id="action_college_fee_type" model="ir.actions.act_window">
        <field name="name">Fee Types</field>
        <field name="res_model">college.fee.type</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>