<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="seq_college_fees" model="ir.sequence">
        <field name="name">College Fees Sequence</field>
        <field name="code">college.fees</field>
        <field name="prefix">FEES/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_college_appointment" model="ir.sequence">
        <field name="name">College Student Appointment Sequence</field>
        <field name="code">college.student.appointment</field>
        <field name="prefix">APP/%(year)s/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_college_student_admission" model="ir.sequence">
        <field name="name">Student Admission Sequence</field>
        <field name="code">college.student</field>
        <field name="prefix">STU/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>